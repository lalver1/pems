POSTGRES_SSLMODE=disable

PGWEB_PORT=8081

# Streamlit
STREAMLIT_LOCAL_PORT=8501
# options: hidden, sidebar
STREAMLIT_NAV=hidden

# AWS
AWS_PROFILE=pems
